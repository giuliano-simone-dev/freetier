{
  "name": "freetier",
  "productName": "FreeTier",
  "description": "Electron Svelte",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": ".vite/build/main.js",
  "author": {
    "name": "Giuliano Simone",
    "email": "giulianosimone9@gmail.com"
  },
  "scripts": {
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run && npm run test:e2e",
    "test:e2e": "playwright test",
    "start": "electron-forge start",
    "postbuild": "node ./scripts/fix-relative-paths.cjs",
    "package": "vite build && npm run postbuild && electron-forge package",
    "make": "vite build && npm run postbuild && electron-forge make",
    "rebuild": "electron-rebuild -f -w better-sqlite3",
    "publish": "electron-forge publish",
    "force-build": "vite build && npm run postbuild",
    "force-forge": "electron-forge package"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-dmg": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron-forge/plugin-vite": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@playwright/test": "^1.49.1",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@tailwindcss/vite": "^4.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/node": "^22",
    "electron": "^36.2.1",
    "electron-rebuild": "^3.2.9",
    "jsdom": "^26.0.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.2.6",
    "vitest": "^3.0.0"
  },
  "dependencies": {
    "better-sqlite3": "^12.2.0",
    "electron-serve": "^2.1.1",
    "electron-squirrel-startup": "^1.0.1",
    "flowbite": "^3.1.2",
    "flowbite-svelte": "^1.10.22",
    "flowbite-svelte-icons": "^2.2.1",
    "svelte-dnd-action": "^0.9.64"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "electron",
      "electron-winstaller"
    ],
    "patchedDependencies": {
      "@sveltejs/kit": "patches/@sveltejs__kit.patch"
    }
  }
}
